import java.util.Scanner;

class main {
  public static void main(String[] args) {
    
	Integer valor1;
    Integer valor2;
    double divisao;
    Scanner resposta = new Scanner(System.in);
  
    System.out.println("Digite o primeiro valor da divisão ");
    valor1 = resposta.nextInt();
    System.out.println("Digite o segundo valor da divisão ");
    valor2 = resposta.nextInt();
    
    divisao = valor1/valor2;
    
    System.out.println("O resultado da sua divisão é igual a:" + divisao);
    
    
    
    
    
  }
}
